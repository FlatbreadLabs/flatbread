{
  "name": "@flatbread/transformer-yaml",
  "version": "1.0.0-alpha.6",
  "description": "Convert YAML files to an in-memory JSON model",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonyketcham/flatbread.git",
    "directory": "packages/transformer-yaml"
  },
  "homepage": "https://github.com/tonyketcham/flatbread/tree/main/packages/transformer-yaml#readme",
  "author": "Tony Ketcham <ketcham.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonyketcham/flatbread/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@flatbread/core": "workspace:*",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.45",
    "tsup": "6.1.3",
    "typescript": "4.7.4",
    "vfile": "5.3.4"
  }
}
