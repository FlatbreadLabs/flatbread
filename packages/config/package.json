{
  "name": "@flatbread/config",
  "version": "1.0.0-alpha.4",
  "description": "Flatbread's user config processing",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonyketcham/flatbread.git",
    "directory": "packages/config"
  },
  "homepage": "https://github.com/tonyketcham/flatbread/tree/main/packages/config#readme",
  "author": "Tony Ketcham <ketcham.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonyketcham/flatbread/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "devDependencies": {
    "@flatbread/core": "workspace:^1.0.0-alpha.10",
    "@types/node": "^16.11.7",
    "tsup": "^5.6.0",
    "typescript": "^4.4.4"
  }
